{
  "name": "react-redux",
  "version": "1.0.0",
  "description": "React Redux Application",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config configs/webpack.dev.js",
    "build:dev": "webpack --config configs/webpack.dev.js -d",
