    "@types/react-router-dom": "^5.1.3",
    "@types/redux": "3.6.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "4.2.2",
    "redux": "^4.0.5",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.9.1",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "expose-loader": "^0.7.5",
    "file-loader": "^5.1.0",
